{
    "name": "pazure/guzzle-azure",
    "type": "library",
    "description": "Azure plugin for the guzzle php http client.",
    "keywords": ["framework", "azure", "api"],
    "license": "MIT",
    "authors": [
        {
            "name": "Gordon Franke",
            "email": "info@nevalon.de"
        },
        {
            "name": "Guzzle Azure Community",
            "homepage": "https://github.com/gimler/guzzle-azure/contributors"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "guzzlehttp/guzzle": "5.3.0",
        "guzzlehttp/guzzle-services": "0.5.0",
        "webbj74/jsdl-loader": "dev-master"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/webbj74/jsdl-loader"
        }
    ],
  "autoload": {
    "psr-0": {
      "Guzzle\\Azure": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Guzzle\\Azure\\Tests\\": "tests/"
    }
  }
}
